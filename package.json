{
  "name": "curved-cycle",
  "version": "4.2.0",
  "scripts": {
    "dev": "pnpm run dev:overseas",
    "dev:base": "astro dev",
    "dev:overseas": "cross-env PUBLIC_SITE_LINE=overseas pnpm run dev:base",
    "dev:cn": "cross-env PUBLIC_SITE_LINE=domestic pnpm run dev:base",
    "start": "pnpm run dev",
    "build": "pnpm run build:overseas",
    "build:base": "astro check && astro build && jampack ./dist",
    "build:overseas": "cross-env PUBLIC_SITE_LINE=overseas pnpm run build:base",
    "build:cn": "cross-env PUBLIC_SITE_LINE=domestic pnpm run build:base",
    "preview": "pnpm run preview:overseas",
    "preview:base": "astro preview",
    "preview:overseas": "cross-env PUBLIC_SITE_LINE=overseas pnpm run preview:base",
    "preview:cn": "cross-env PUBLIC_SITE_LINE=domestic pnpm run preview:base",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "husky install && patch-package",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/rss": "^4.0.2",
    "@resvg/resvg-js": "^2.6.0",
    "astro": "^4.2.1",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@astrojs/react": "^3.0.9",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@divriots/jampack": "^0.23.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/github-slugger": "^1.3.0",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.19.0",
    "astro-eslint-parser": "^0.16.2",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "patch-package": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix",
      "prettier --write --plugin=prettier-plugin-astro"
    ],
    "*.{md,mdx,json}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  }
}
